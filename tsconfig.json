{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES2022",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2022", "DOM"],
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
